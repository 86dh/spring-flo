{
  "name": "spring-flo",
  "version": "0.12.0",
  "description": "Library for quickly building text DSL visualization diagram editor",
  "author": "Andy Clement, Alex Boyko",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/spring-projects/spring-flo.git"
  },
  "engines": {
    "node": ">= 6.9.0",
    "npm": ">= 3.0.0"
  },
  "dependencies": {
    "codemirror-minified": "5.54.0",
    "jointjs": "3.4.1",
    "ts-disposables": "2.2.3"
  },
  "peerDependencies": {
    "@angular/core": ">=11.0.0 ",
    "@angular/forms": ">=11.0.0",
    "@angular/common": ">=11.0.0",
    "rxjs": ">=6.0.0"
  },
  "ngPackage": {
    "lib": {
      "entryFile": "./index.ts",
      "umdModuleIds": {
        "jointjs": "joint",
        "jquery": "$",
        "lodash": "_",
        "ts-disposables": "tsDisposables",
        "codemirror-minified": "codemirror"
      }
    },
    "whitelistedNonPeerDependencies": [
      "codemirror-minified",
      "jointjs",
      "ts-disposables"
    ],
    "dest": "../../dist",
    "deleteDestPath": true
  }
}
